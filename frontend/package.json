  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:unit": "react-scripts test --testPathPattern=__tests__ --watchAll=false",
    "test:integration": "react-scripts test --testPathPattern=integration --watchAll=false",
    "test:mobile": "react-scripts test --testPathPattern=mobile --watchAll=false",
    "test:accessibility": "react-scripts test --testPathPattern=accessibility --watchAll=false",
    "test:performance": "react-scripts test --testPathPattern=performance --watchAll=false",
    "test:watch": "react-scripts test --watch",
    "test:all": "node scripts/run-tests.js",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "type-check-strict": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react-router-dom": "^5.3.3",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.1"
  },
  "proxy": "http://localhost:8000",
  "resolutions": {
    "@types/d3-dispatch": "3.0.7"
  },
  "overrides": {
    "@types/d3-dispatch": {
      "typescript": ">=4.9.0"
    }
  }
