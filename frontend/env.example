# Frontend Environment Variables
# Copy this file to .env and update the values as needed

# API Configuration
# For local development, point to local backend:
# REACT_APP_API_URL=http://127.0.0.1:8000/api/v1
# For staging/production, point to your domain:
# REACT_APP_API_URL=https://your-domain.example.com/api/v1
# If unset, frontend will use same-origin '/api/v1' via reverse proxy

# Development settings
REACT_APP_ENV=development
REACT_APP_DEBUG=true 